{
  "ConnectionStrings": {
    "DefaultConnection": "data source={Host};initial catalog=InvoiceDb;user id={DbUserName};password={DbPassword};App=Invoice"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtIssuerOptions": {
    "Issuer": "InvoiceAPI",
    "Audience": "http://localhost:60993/",
    "PrivateKey": "",
    "PublicKey": "",
    "TrustedPublicKeys": [
      ""
    ],
    "TrustedIssuers": [
      "InvoiceApp"
    ]
  }
}
